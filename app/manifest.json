{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "version": "1.0.1",
  "default_locale": "en",

  "sidebar_action": {
    "default_icon": "icons/star.png",
    "default_panel": "index.html"
  },

  "__firefox__commands": {
    "_execute_sidebar_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+N"
      }
    }
  },

  "permissions": ["storage"]
}
